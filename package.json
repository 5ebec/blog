{
  "name": "blog",
  "version": "0.1.0",
  "description": "heyhoe's pothooks",
  "repository": "https://github.com/5ebec/blog",
  "author": "5ebec <hash.a5ebec@gmail.com>",
  "license": "MIT",
  "private": true,
  "browserslist": [
    "last 2 versions",
    "> 2%",
    "not dead"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "prod": "run-s prod:*",
    "dev:css": "postcss theme/css/_style.pcss --dir _css --ext css -w",
    "prod:css": "postcss theme/css/_style.pcss --dir _css --ext css --no-map",
    "dev:franklin": "julia --project=. -e 'using Franklin;serve(clear=true)'",
    "prod:franklin": "julia --project=. -e 'using Franklin;optimize()'",
    "format": "prettier --write . -u"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2"
  },
  "dependencies": {
    "postcss": "^8.1.8",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.7.0",
    "highlight.js": "^10.4.0"
  }
}
